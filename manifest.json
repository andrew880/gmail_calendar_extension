{
  "name": "note",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/external/jquery-3.5.1.min.js", "/external/sweetalert.min.js", "/external/jquery-ui.min.js", "/functions/stickynote.js","content.js"],
      "css": ["/style/css/styles.css", "/style/css/sweetalert.css"]
    }
  ],
  "browser_action": {
    "default_title": "Momo"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
